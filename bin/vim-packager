#!/usr/bin/env perl 
use warnings;
use strict;
use Vim::Packager;
use Vim::Pacakger::Command;

$|++;
my $cmd = shift @ARGV;
my $ret = Vimana::Packager::Command->invoke( $cmd, @ARGV );

exit (defined $ret ? $ret : 1);

1;
__END__

